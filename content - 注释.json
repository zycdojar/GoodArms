{
    "name": "qh1",<====这个是方案名称，必须在config.yml里面的list里面存在
    "level": {
        "max": 120<=====最大等级
    },
	"lore": [<==========使用这个方案后剑上面的lore
        "&7===============================",
        "&c&l[&b基础伤害&c&l] &2*hurt*",
        "&c&l[&b暴击率&c&l] &2*crit*",
        "&c&l[&b暴击效果&c&l] &2*critEffect*",
        "&c&l[&b格挡&c&l] &2*resist*",
        "&c&l[&b等级&c&l] &2*level*",
        "&7==============================="
    ],
    "update": {
        "material": {<=====每级耗费的材料
            "level1": {<=====level+等级
                "1024": 10,<=====1024为物品id,10为数量
                "money": 10<=====money为金钱,10为数量
            },
            "level2": {
                "1024": 10,
                "money": 10
            }
        },
        "attribute": {<=====属性：(不包括原版伤害)暴击时: 伤害=基础伤害+(基础伤害×暴击效果÷100)]
            "level1": {<=====level+等级
                "hurt": 10,<=====基础伤害
                "crit": 5,<=====暴击率
                "critEffect": 10,<=====暴击效果
				"resist": 10<======格挡(挡去后面的数值伤害)
            },
            "level2": {
                "hurt": 20,
                "crit": 20,
                "critEffect": 20
            }
        },
        "normal": {<=======当上面属性没有配置某个等级时，这个每级按: 当前等级属性+这里面配置的属性
            "hurt": 0.5,
            "crit": 0.01,
            "critEffect": 0.001,
            "material": {<=========当上面耗费材料没有配置某个等级时，这个每级这里面配置的
                "1024": 10,
                "money": 10
            }
        }
    },
    "first": {<======最初的属性(不包括原版伤害)
        "hurt": 50,
        "crit": 1,
        "critEffect": 1
    }
}